{
  "name": "relay-playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^0.0.0-experimental-895a6fe0",
    "relay-runtime": "^9.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "relay:fetch_schema": "curl --location --output data/schema.graphql https://raw.githubusercontent.com/graphql/swapi-graphql/master/schema.graphql",
    "relay": "relay-compiler --src ./src --schema data/schema.graphql --language typescript",
    "tsc": "tsc --noEmit"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "react-scripts/config/jest/babelTransform.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__generated__/"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-relay": "^7.0.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/relay-runtime": "^8.0.11",
    "@types/relay-test-utils": "^6.0.2",
    "babel-plugin-relay": "^9.1.0",
    "graphql": "^15.0.0",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "relay-compiler": "^9.1.0",
    "relay-compiler-language-typescript": "^12.0.3",
    "relay-config": "^9.1.0",
    "relay-test-utils": "^9.1.0",
    "typescript": "^3.9.3"
  },
  "prettier": {}
}
